{
  "name": "http-wire",
  "version": "0.0.1",
  "description": "parse http at http1.1",
  "license": "ISC",
  "author": "quanxiaoxiao",
  "type": "module",
  "main": "dist/index.js",
  "scripts": {
    "build": "tsc && rm -rf dist/*.test.* dist/**/*.test.*",
    "check": "tsc --noEmit",
    "format": "prettier --write .",
    "lint": "eslint src --ext .ts",
    "test": "TSX_TSCONFIG=tsconfig.test.json node --import tsx --test 'src/**/*.test.ts'",
    "test:message": "TSX_TSCONFIG=tsconfig.test.json node --import tsx --test 'src/decode/message.test.ts'",
    "test:coverage": "TSX_TSCONFIG=tsconfig.test.json node --import tsx --test --experimental-test-coverage 'src/**/*.test.ts'",
    "test:single": "TSX_TSCONFIG=tsconfig.test.json node --import tsx --test --test-only",
    "test:fuzz": "TSX_TSCONFIG=tsconfig.test.json node --import tsx --test 'src/decode/fuzz.test.ts'"
  },
  "publishConfig": {
    "registry": "https://registry.npmjs.org"
  },
  "engines": {
    "node": ">= 22.0.0"
  },
  "devDependencies": {
    "@types/node": "^25.0.0",
    "eslint": "^9.39.1",
    "eslint-plugin-import": "^2.32.0",
    "eslint-plugin-simple-import-sort": "^12.1.1",
    "eslint-plugin-unicorn": "^62.0.0",
    "globals": "^16.5.0",
    "ts-node": "^10.9.2",
    "tsx": "^4.21.0",
    "typescript": "^5.9.3",
    "typescript-eslint": "^8.47.0"
  }
}
